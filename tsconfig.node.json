{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": ["electron.vite.config.*", "src/main/*", "src/main/**/*", "src/preload/*"],
  "compilerOptions": {
    "baseUrl": "./",
    "types": ["electron-vite/node"],
    "paths": {
      "@shared/*": [
        "src/shared/*"
      ],
      "@dev": [
        "src/main/dev"
      ]
    }
  }
}
