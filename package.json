{
  "name": "AnimExperience",
  "version": "1.0.0",
  "description": "Watch anime easily",
  "main": "./dist/main.js",
  "engines": {
    "node": "14.18.0"
  },
  "scripts": {
    "gserve": "gatsby serve",
    "dev": "concurrently \"yarn electron:serve\" \"yarn dev:react\"",
    "dev:electron": "cross-env NODE_ENV=development webpack --config webpack/electron.webpack.js --mode development && yarn start:electron",
    "dev:react": "cross-env NODE_ENV=development gatsby develop",
    "electron:serve": "wait-on http://localhost:8000 && yarn dev:electron",
    "start:electron": "electron .",
    "build": "npm-run-all build:react build:electron",
    "build:run": "npm-run-all build start:electron",
    "build:electron": "webpack --config webpack/electron.webpack.js",
    "build:react": "gatsby build",
    "package": "npm-run-all build package:dist",
    "package:dist": "electron-builder",
    "test": "jest",
    "test:o": "jest --watch",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Abraham Pacheco <abraham.pacheco6319@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@commitlint/core": "^9.0.1",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^26.0.3",
    "@types/jquery": "^3.5.5",
    "@types/node-fetch": "^2.5.10",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/react-redux": "^7.1.16",
    "@types/react-transition-group": "^4.4.1",
    "@types/redux-mock-store": "^1.0.3",
    "@types/styled-components": "^5.1.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "babel-loader": "^8.2.2",
    "babel-preset-gatsby": "^1.13.0",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.2",
    "css-loader": "^5.2.6",
    "cz-conventional-changelog": "3.2.0",
    "electron": "^20.1.0",
    "electron-builder": "23.6.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-rebuild": "^2.3.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-webpack-plugin": "^3.0.1",
    "file-loader": "^6.2.0",
    "gatsby-plugin-eslint": "^4.0.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "jest-styled-components": "^7.1.1",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "raw-loader": "^4.0.2",
    "redux-mock-store": "^1.5.4",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.5",
    "wait-on": "^5.3.0",
    "webpack": "^5.37.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/polyfill": "^7.12.1",
    "@babel/runtime": "^7.14.0",
    "@cliqz/adblocker-electron": "^1.20.6",
    "@electron/remote": "^2.0.8",
    "@mui/icons-material": "^5.0.0",
    "@mui/material": "^5.0.0",
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@^5.0.0",
    "@mui/styles": "^5.0.0",
    "@reach/rect": "^0.15.0",
    "@reduxjs/toolkit": "^1.9.3",
    "cheerio": "^1.0.0-rc.9",
    "concurrently": "^6.2.1",
    "copy-webpack-plugin": "^9.0.0",
    "electron-serve": "^1.1.0",
    "electron-store": "^8.0.0",
    "filewatcher": "^3.0.1",
    "gatsby": "^3.13.0",
    "gatsby-plugin-ipfs": "^2.0.2",
    "gatsby-plugin-less": "^5.13.0",
    "gatsby-plugin-material-ui": "^3.0.1",
    "gatsby-plugin-output": "^0.1.3",
    "gatsby-plugin-styled-components": "^4.13.0",
    "gatsby-plugin-typescript": "^3.13.0",
    "gatsby-theme-material-ui": "^2.0.1",
    "jquery": "^3.6.0",
    "json5": "^2.2.0",
    "less": "^4.1.1",
    "less-loader": "^9.0.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "node-fetch": "^2.6.1",
    "node-loader": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-transition-group": "^4.4.1",
    "react-waypoint": "^10.1.0",
    "redux": "^4.1.0",
    "sass": "^1.34.0",
    "sass-loader": "^11.1.1",
    "styled-components": "^5.1.1",
    "tslog": "^3.2.2",
    "use-resize-observer": "^7.0.0",
    "uuid": "^8.3.2"
  },
  "resolutions": {
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@^5.0.0"
  },
  "build": {
    "win": {
      "target": [
        "msi",
        "nsis"
      ]
    },
    "appId": "com.electron.animexperience",
    "mac": {
      "category": "public.app-category.video"
    },
    "directories": {
      "output": "packages"
    },
    "files": [
      "package.json",
      "dist/**"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
