{
  "name": "animexperience",
  "version": "1.0.5",
  "description": "Watch anime easily",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "license": "MIT",
  "engines": {
    "node": "18"
  },
  "repository": "https://github.com/jeweells/animexperience",
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "yarn typecheck:node && yarn typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "dev:w": "yarn dev --watch",
    "build": "yarn typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "yarn build && electron-builder --dir",
    "build:win": "yarn build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "test": "jest",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@cliqz/adblocker-electron": "^1.26.15",
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.6",
    "@mui/material": "^5.15.6",
    "@mui/styles": "^5.15.6",
    "@reach/rect": "^0.18.0",
    "@reduxjs/toolkit": "^2.1.0",
    "cheerio": "^1.0.0-rc.12",
    "electron-devtools-installer": "^3.2.0",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.7",
    "filewatcher": "^3.0.1",
    "jquery": "^3.7.1",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.44",
    "node-fetch": "^2.6.11",
    "react-redux": "^9.1.0",
    "react-transition-group": "^4.4.5",
    "react-waypoint": "^10.3.0",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^3.1.0",
    "tslog": "^4.9.2",
    "use-resize-observer": "^9.1.0",
    "uuid": "^9.0.1",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/jest": "^11.11.0",
    "@testing-library/jest-dom": "^6.3.0",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/jquery": "^3.5.29",
    "@types/node": "^18.19.9",
    "@types/node-fetch": "^2.6.11",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/redux-mock-store": "^1.0.6",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-plugin-tsconfig-paths": "^1.0.3",
    "babel-preset-react-app": "^10.0.1",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^9.0.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  },
  "lint-staged": {
    "*.{js,jsx,cjs,mjs,ts,tsx,cts,mts}": [
      "eslint --fix"
    ]
  }
}
